<p id="notice"><%= notice %></p>

<h1>Listing Congoers</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @congoers.each do |congoer| %>
      <tr>
        <td><%= congoer.description %></td>
        <td><%= link_to 'Show', congoer %></td>
        <td><%= link_to 'Edit', edit_congoer_path(congoer) %></td>
        <td><%= link_to 'Destroy', congoer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Congoer', new_congoer_path %>
